<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.control.*?>
<?import java.net.URL?>
<TableView xmlns:fx="http://javafx.com/fxml/1" styleClass="mod-table" fx:id="root"
           xmlns="http://javafx.com/javafx/8.0.172-ea">
	<stylesheets>
		<URL value="@../main.css"/>
		<URL value="@../table-view.css"/>
	</stylesheets>
	<columns>
		<TableColumn text="Mod Name">
			<cellValueFactory>
				<PropertyValueFactory property="modName"/>
			</cellValueFactory>
		</TableColumn>
		<TableColumn text="MC Version">
			<cellValueFactory>
				<PropertyValueFactory property="minecraftVersion"/>
			</cellValueFactory>
		</TableColumn>
		<TableColumn text="Mod Id">
			<cellValueFactory>
				<PropertyValueFactory property="modId"/>
			</cellValueFactory>
		</TableColumn>
		<TableColumn text="File Name">
			<cellValueFactory>
				<PropertyValueFactory property="fileName"/>
			</cellValueFactory>
		</TableColumn>
	</columns>
</TableView>